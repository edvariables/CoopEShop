	edvInfo = :Name + " : Chargement des paramètres généraux.";
	var dataTable = ...;
	var lVarRoot =..edvAppli:Parent;
	if (lVarRoot != null){
		for(var lRow : DataRow in dataTable.Select("", "SortIndex").GetEnumerator()) {
